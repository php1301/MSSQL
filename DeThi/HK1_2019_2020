SELECT HOTEN, NGSINH, GIOITINH 
FROM VANDONGVIEN
WHERE QUOCTICH='UK'
ORDER BY HOTEN ASC

SELECT VDV.MAVDV
FROM VANDONGVIEN VDV THAMGIA TG NOIDUNGTHI NDT
WHERE VDV.MAVDV = TG.MAVDV AND TG.MANDT = NDT.MANDT AND NDT.TENNDT='BANCUNG'

SELECT MAVDV, COUNT(HuyChuong)
FROM VANDONG VDV THAMGIA TG  THEVANHOI TVH
WHERE VDV.MAVDV = TG.MAVDV AND VDV.QUOCTINH='NHATBAN' AND TVH.MATVH = TG.MATVH AND TVH.NAM=2020
GROUP BY MAVDV

SELECT HOTEN, QUOCTINH
FROM VANDONGVIEN VDV THAMGIA TG NOIDUNGTHI NDT 
WHERE VDV.MAVDV = TG.MAVDV AND NDT.MANDT = TG.MANDT AND NDT.NOIDUNGTHI='100MBOINGUA'

INTERSECT

SELECT HOTEN, QUOCTINH
FROM VANDONGVIEN VDV THAMGIA TG NOIDUNGTHI NDT 
WHERE VDV.MAVDV = TG.MAVDV AND NDT.MANDT = TG.MANDT AND NDT.NOIDUNGTHI='200MBOITUDO'



Tim nhung van dong vien nu quoc tinh UK tham gia tat ca the van hoi tu 2008 toi nay

SELECT MAVDV, HOTEN
FROM VANDONGVIEN VDV 
WHERE NOT EXISTS (
    SELECT * THEVANHOI TVH 
    WHERE TVH.NAM > 2008
    AND NOT EXISTS (
        Select * FROM THAMGIA TG 
        WHERE TG.MAVDV = VDV.MAVDV AND VDV.QUOCTICH="UK" VDV.GIOITINH="NU"
    )
)



Tim nhung van dong vien tham gia the van hoi ten 'Olympic Rio 2016' dat duoc huy chuong vang (HUYCHUONG=2) >=2

SELECT VDV.MAVDV, HOTEN, COUNT(TVH.HUYCHUONG)
FROM VANDONGVIEN VDV, THAMGIA TG, THEVANHOI TVH
WHERE VDV.MAVDV = TG.MAVDV AND TG.MATVH = TVH.MATVH AND TVH.TENTVH = 'Olympic Rio 2016' and TVH.HUYCHUONG=2
GROUP BY MAVDV, HOTEN
HAVING COUNT(TVH.HUYCHUONG) >= 2